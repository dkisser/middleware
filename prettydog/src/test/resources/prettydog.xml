<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:prettydog="http://wc.prettydog.org/schema/prettydog"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
       http://wc.prettydog.org/schema/prettydog http://wc.prettydog.org/schema/prettydog-1.0.0.xsd">

    <!--<bean name="testInterface" class="org.wc.prettydog.util.itest.TestInterfaceImpl1"/>-->
    <!--<prettydog:bean name="testInterface" class="org.wc.prettydog.util.itest.TestInterface"-->
                    <!--impl="test1" ></prettydog:bean>-->
    <bean id="testAdvisor" class="org.wc.prettydog.util.itest.aop.TestInterfaceAdvisor"/>
    <bean id="testAdvisor1" class="org.wc.prettydog.util.itest.aop.TestInterfaceAdvisor1"/>
    <bean id="testAop" class="org.springframework.aop.framework.ProxyFactoryBean">
        <property name="proxyInterfaces">
            <value>org.wc.prettydog.util.itest.TestInterface</value>
        </property>
        <property name="target">
            <bean class="org.wc.prettydog.util.itest.TestInterfaceImpl1"/>
        </property>
        <property name="interceptorNames">
            <list>
                <value>testAdvisor</value>
            </list>
        </property>
    </bean>
    <bean id="testAop2" class="org.springframework.aop.framework.ProxyFactoryBean">
        <property name="target">
            <bean class="org.wc.prettydog.util.itest.aop.Printer"/>
        </property>
        <property name="interceptorNames">
            <list>
                <value>testAdvisor</value>
                <value>testAdvisor1</value>
            </list>
        </property>
    </bean>
</beans>